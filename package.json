{
  "name": "@addepar/ember-toolbox",
  "version": "0.8.0-alpha.3",
  "description": "Shared styles and helper for ICE components",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "github:Addepar/addepar-ember-toolbox",
  "scripts": {
    "build": "ember build",
    "lint": "ember adde-lint",
    "lint:files": "ember adde-lint --file-names",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "ember adde-lint --javascript",
    "lint:sass": "ember adde-lint --sass",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "^7.7.3",
    "eslint": "^4.19.1",
    "execa": "^5.0.0",
    "fs-extra": "^5.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^1.19.1",
    "sass-lint": "^1.12.1",
    "walk-sync": "^0.3.2"
  },
  "devDependencies": {
    "@addepar/eslint-config": "^5.0.0",
    "@addepar/prettier-config": "^1.0.0",
    "@addepar/sass-lint-config": "^2.0.1",
    "@addepar/style-toolbox": "^0.4.0",
    "@ember/optional-features": "^0.7.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.10.1",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-esdoc": "~0.0.2",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-sass-lint": "^1.0.4",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~2.16.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^6.2.0",
    "eslint-plugin-node": "^11.0.0",
    "husky": "^4.2.1",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.4"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "ember-cli-sass"
  },
  "publishConfig": {
    "@addepar:registry": "https://registry.npmjs.org/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ember adde-pre-commit"
    }
  },
  "bin": {
    "adde-pre-commit": "./bin/adde-pre-commit.js",
    "adde-lint-file-names": "./bin/adde-lint-file-names.js"
  }
}
